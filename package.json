{
  "name": "@bunary/cli",
  "version": "0.0.3",
  "description": "CLI scaffolding tool for Bunary - a Bun-first backend framework inspired by Laravel",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "bunary": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target bun && bunx tsc -p tsconfig.build.json",
    "test": "bun test",
    "lint": "bunx biome check ./src ./tests",
    "format": "bunx biome check --write ./src ./tests",
    "typecheck": "bunx tsc --noEmit"
  },
  "keywords": [
    "bunary",
    "bun",
    "cli",
    "scaffolding",
    "generator",
    "framework"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bunary-dev/cli.git"
  },
  "homepage": "https://github.com/bunary-dev/cli#readme",
  "bugs": {
    "url": "https://github.com/bunary-dev/cli/issues"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "dependencies": {
    "@bunary/core": "^0.0.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bun": "1.3.6",
    "typescript": "^5.9.3"
  }
}
